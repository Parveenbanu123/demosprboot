<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" 
xmlns="https://java.sun.com/xml/ns/j2ee" 
xmlns:web="https://java.sun.com/xml/ns/javaee" 
xsi:schemaLocation="https://java.sun.com/xml/ns/javaee https://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
version="2.5">


  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <description></description>
    <display-name>FirstServ</display-name>
    <servlet-name>FirstServ</servlet-name>
    <servlet-class>com.mph.FirstServ</servlet-class>
    <init-param>
      <param-name>un</param-name>
      <param-value>Parveen</param-value>
    </init-param>
    <init-param>
      <param-name>pwd</param-name>
      <param-value>pass</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>FirstServ</servlet-name>
    <url-pattern>/FS</url-pattern>
  </servlet-mapping>
  
  <filter>
    <description></description>
    <display-name>MyFilter</display-name>
    <filter-name>MyFilter</filter-name>
    <filter-class>com.mph.MyFilter</filter-class>
    <init-param>
    <param-name>fun</param-name>
    <param-value>Parveen</param-value>
    </init-param>
     <init-param>
    <param-name>fpwd</param-name>
    <param-value>Parveen</param-value>
    </init-param>
    
  </filter>
  <filter-mapping>
    <servlet-name>MyFilter</servlet-name>
    <url-pattern>/MyFilter</url-pattern>
  </filter-mapping>
</web-app>
